	function test(name, url)
	{
		$(function()
		{
			url += "&app_code=" + app_code + "&app_id=" + app_id;
			// For catalog serivce app_token is required
			if (url.indexOf("navigation-voice") > -1) {
				url = url.replace("app_code", "app_token");
			}
			$.ajax({
					url: url,
					beforeSend: function(jqxhr, settings){
						jqxhr.requestURL = url;
					},
					 dataType: "jsonp",
					jsonpCallback: name,
					
					error: function(request, status, error, n){
						
						var d = request.requestURL.substring(request.requestURL.indexOf("back="));
						d = d.substring(d.indexOf("=") + 1, d.indexOf("&"));
						var obj = document.getElementById(d);
						if (!obj) {
							d = request.requestURL.substring(request.requestURL.indexOf("func="));
							d = d.substring(d.indexOf("=") + 1, d.indexOf("&"));
							obj = document.getElementById(d);
						}
						
						// Catalog service results in parse error but gives an http 200
						if(request.status== 200 && request.requestURL.indexOf("navigation-voice")>-1){
							var sucess = new Object();
							sucess.response = "Available";
							updateTable(obj, sucess);
						}
						else{
							var e = new Object();
							e.error = status;
							updateTable(obj, e);
						}
						
					}
				}); 
		});
	}
	
	function getUrlParameter(param)
	{
		var pageURL = window.location.search.substring(1),
			urlVariables = pageURL.split('&');

		for (var i = 0; i < urlVariables.length; i++){

			var parameterName = urlVariables[i].split('=');
			if (parameterName[0] == param){
				return parameterName[1];
			}
		}
		return null;
	}

	if(getUrlParameter("app_id") && getUrlParameter("app_code"))
	{
		document.getElementById('appIdExt').value = getUrlParameter("app_id");
		document.getElementById('appCodeExt').value = getUrlParameter("app_code")
		init();
	}

