	function updateTable(obj, resp)
	{
		document.getElementById("result_table").style.display = "block";
		document.getElementById("check").disabled = true;
		document.getElementById('appIdExt').disabled = true;
		document.getElementById('appCodeExt').disabled = true;
		
		var ok = true; 
		if(resp && resp.type && (resp.type.indexOf("Error") != -1 || resp.type.indexOf("error") != -1 ))
			ok = false;
		if(resp && resp.response && resp.response.type && (resp.response.type.indexOf("SystemError") != -1 || resp.response.type.indexOf("PermissionError") != -1))
			ok = false;
		if(resp && resp.error)
			ok = false;
		if(resp && resp.errors && resp.errors.length != 0)
			ok = false;
		
		obj.innerHTML = ok ? svg_ok : svg_no;
	}
	
	

